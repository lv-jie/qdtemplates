            "mimeType": "application/json"
        },
        "rule": {
            "success_asserts": [
                {
                    "re": "200",
                    "from": "status"
                }
            ],
            "failed_asserts": [
                {
                    "re": "您的账号\\/密码错误，请重新输入",
                    "from": "content"
                }
            ],
            "extract_variables": [
                {
                    "name": "token",
                    "re": "\"token\":\"(.+)\"",
                    "from": "content"
                }
            ]
        }
    },

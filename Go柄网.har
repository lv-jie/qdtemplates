                },
                {
                    "name": "sec-ch-ua-platform",
                    "value": "\"Windows\""
                },
                {
                    "name": "Origin",
                    "value": "https://www.gobing.cn"
                },
                {
                    "name": "Sec-Fetch-Site",
                    "value": "same-site"
                },
                {
                    "name": "Sec-Fetch-Mode",
                    "value": "cors"
                },
                {
                    "name": "Sec-Fetch-Dest",
                    "value": "empty"
                },
                {
                    "name": "Referer",
                    "value": "https://www.gobing.cn/index"
                },
                {
                    "name": "Accept-Encoding",
                    "value": "gzip, deflate, br"
                },
                {
                    "name": "Accept-Language",
                    "value": "zh-CN,zh;q=0.9"
                }
            ],
            "cookies": [],
